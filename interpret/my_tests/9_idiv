.IPPcode23

DEFVAR GF@int_1
DEFVAR GF@int_2
DEFVAR GF@return

# test 1
MOVE GF@int_1 int@1
MOVE GF@int_2 int@1
IDIV GF@return GF@int_1 GF@int_2
WRITE GF@return                         # = 1
WRITE string@\010

# test 2
MOVE GF@int_2 int@-1                    
IDIV GF@return GF@int_1 GF@int_2
WRITE GF@return                         # = -1
WRITE string@\010

# test 3 max int, min int          
MOVE GF@int_1 int@5000000000
MOVE GF@int_2 int@5
IDIV GF@return GF@int_1 GF@int_2
WRITE GF@return                         # = 1000000000
WRITE string@\010                   

# test 4 small and big int
MOVE GF@int_1 int@20
MOVE GF@int_2 int@10
IDIV GF@return GF@int_1 GF@int_2
WRITE GF@return                         # = 200
WRITE string@\010

# test 5 big and small int
IDIV GF@return int@10 int@20
WRITE GF@return                         # = 0
WRITE string@\010

# test zero and int
MOVE GF@int_1 int@0
IDIV GF@return GF@int_1 int@20
WRITE GF@return                         # = 0
WRITE string@\010

# error test
IDIV GF@return int@20 int@0
WRITE GF@return                         # = error 57
WRITE string@\010

