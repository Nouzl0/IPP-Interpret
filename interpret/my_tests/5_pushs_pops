.IPPcode23

# stack test
DEFVAR GF@hello
DEFVAR GF@world
MOVE GF@hello string@hello
MOVE GF@world string@world
PUSHS GF@hello
PUSHS GF@world
PUSHS GF@hello
PUSHS GF@world

# test 1
DEFVAR GF@return
POPS GF@return
WRITE GF@return
WRITE string@\010       # = world

# test 2
POPS GF@return
WRITE GF@return
WRITE string@\010       # = hello

# test 3
POPS GF@return
WRITE GF@return
WRITE string@\010       # = world

# test 4
POPS GF@return
WRITE GF@return
WRITE string@\010       # = hello

# test 5
PUSHS int@1
POPS GF@return
WRITE GF@return
WRITE string@\010       # = 1

# test 6
PUSHS int@-565
POPS GF@return
WRITE GF@return
WRITE string@\010       # = -565

# test 7
PUSHS nil@nil
POPS GF@return
WRITE GF@return    
WRITE string@\010       # = ""

# test 8
PUSHS bool@true
POPS GF@return
WRITE GF@return     
WRITE string@\010       # = true

# test 9
PUSHS bool@false
POPS GF@return
WRITE GF@return      
WRITE string@\010       # = false

# test 10
MOVE GF@return int@3465
PUSHS GF@return
POPS GF@return
WRITE GF@return         
WRITE string@\010       # = 3465